<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    >

    <LinearLayout
        android:id="@+id/title_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:padding="10dp"
        >

        <TextView
            android:id="@+id/tv_net_title"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="网络"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/tv_target_title"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="目标"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/tv_call_type_title"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="呼叫类型"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/tv_call_time_title"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="通话/s"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/tv_result_title"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="结果"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            />

    </LinearLayout>

    <View
        android:id="@+id/line_divider"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/color_test_detail_gray_bg"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="10dp"
        >

        <TextView
            android:id="@+id/tv_net"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="WiFi"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:ellipsize="end"
            />

        <TextView
            android:id="@+id/tv_target"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="淘宝"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:ellipsize="end"
            />

        <TextView
            android:id="@+id/tv_call_type"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="CSFB"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:ellipsize="end"
            />

        <TextView
            android:id="@+id/tv_call_time"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="3.2"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:ellipsize="end"
            />

        <TextView
            android:id="@+id/tv_result"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="成功"
            android:textSize="@dimen/test_list_item_text_size"
            android:textColor="@color/color_test_detail_gray_bg"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:ellipsize="end"
            />

    </LinearLayout>

</LinearLayout>